mixin sBlogNav(data)
	// start sBlogNav
	+b.SECTION.sBlogNav.section#sBlogNav&attributes(attributes)
		+e.sticky-wrap--js
			+e.title Содержание
			<nav class="blog-nav" title="Содеражние статьи">
				ul.blog-nav__list.nav
					li.blog-nav__item
						a(href="#").blog-nav__link Приготовление бензиново-масляной смеси
					li.blog-nav__item
						a(href="#").blog-nav__link Обкатка двигателя&nbsp;&mdash; почему это важно?
					li.blog-nav__item
						a(href="#").blog-nav__link Выбор масла для редуктора
					li.blog-nav__item
						a(href="#").blog-nav__link Обслуживание триммера в&nbsp;домашних условиях
						ul
							li
								a(href="#dvig") Двигатель
							li
								a(href="#") Режущий механизм
							li
								a(href="#") Шпуля с&nbsp;леской
							li
								a(href="#") Редуктор
							li
								a(href="#") Защитный кожух
					li.blog-nav__item
						a(href="#").blog-nav__link Триммер не&nbsp;заводится&nbsp;&mdash; что делать?
					li.blog-nav__item
						a(href="#").blog-nav__link Типичные поломки и&nbsp;способы их&nbsp;устранения
					li.blog-nav__item
						a(href="#").blog-nav__link Как продлить срок службы триммера?
	// end sBlogNav
